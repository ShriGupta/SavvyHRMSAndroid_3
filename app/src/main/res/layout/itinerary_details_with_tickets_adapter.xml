<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="7sp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:orientation="horizontal">

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:text="Source :"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:id="@+id/travelDeskHistoryItinerarySource"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5">

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView

                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:text="Destination"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:id="@+id/travelDeskHistoryItineraryDestination"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:orientation="horizontal">

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:text="Departure Date"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:id="@+id/travelDeskHistoryItineraryDepartureDate"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5">

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:text="Return Date"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:id="@+id/travelDeskHistoryItineraryReturnDate"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5"

                android:orientation="horizontal">

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:text="Mode"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:id="@+id/travelDeskHistoryItineraryMode"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5">

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:text="Class"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:id="@+id/travelDeskHistoryItineraryClass"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5"

                android:orientation="horizontal">

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:text="Type"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:id="@+id/travelDeskHistoryItineraryType"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5">

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:text="Ticket Amount"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />

                <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                    android:id="@+id/travelDeskHistoryTicketAmount"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:textColor="@color/my_custom_color"
                    android:textSize="@dimen/text_size" />
            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:visibility="gone">

            <Button
                android:id="@+id/travelDeskHistoryItineraryDownload"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20sp"
                android:backgroundTint="@color/color_red"
                android:text="Download"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5sp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/color_red"
                android:src="@drawable/line" />

        </LinearLayout>


    </LinearLayout>


</androidx.coordinatorlayout.widget.CoordinatorLayout>