                            android:text="Req. Date"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:textStyle="bold"
                            android:text="App. Amount"
                            android:textColor="@color/heading_dark"
                            android:textSize="@dimen/status_approval"
                            android:gravity="center_vertical"
                            app:font="@string/font_name_light" />

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_conApproveAppAmt"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_toRightOf="@+id/txt_leavenameTitle"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="App. Amount"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_conApproveModify"
                            android:layout_width="0dp"
                            android:layout_height="26dip"
                            android:layout_marginTop="0dp"
                            android:layout_marginRight="50dp"
                            android:textStyle="bold"
                            android:layout_weight="1"
                            android:paddingLeft="10dp"
                            android:visibility="visible"
                            android:paddingRight="10dp"
                            android:gravity="center"
                            android:text="Modify"
                            android:background="@drawable/button_border"
                            android:textColor="#fff"
                            android:textSize="12dp"/>

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_conAction"
                            android:layout_width="0dp"
                            android:layout_height="26dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginLeft="-45dp"
                            android:textStyle="bold"
                            android:layout_weight="1"
                            android:paddingLeft="10dp"
                            android:visibility="visible"
                            android:paddingRight="10dp"
                            android:gravity="center"
                            android:text="Action"
                            android:background="@drawable/button_border"
                            android:textColor="#fff"
                            android:textSize="12dp"/>

                        <!--<RelativeLayout-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="match_parent"-->
                            <!--android:layout_weight="5">-->
