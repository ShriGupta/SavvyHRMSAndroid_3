<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="5"
                android:orientation="vertical"
                android:weightSum="8">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_punch_token"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Token No."
                        android:textStyle="bold"
                        android:textColor="@color/heading_dark"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_conApproveToken"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_toRightOf="@+id/txt_leavenameTitle"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="Token Value"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:textStyle="bold"
                        android:text="E-Name"
                        android:textColor="@color/heading_dark"
                        android:textSize="@dimen/status_approval"
                        android:gravity="center_vertical"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_conApproveEName"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_toRightOf="@+id/txt_leavenameTitle"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="E-Name"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Req. Amount"
                        android:textStyle="bold"
                        android:textColor="@color/heading_dark"
                        android:textSize="@dimen/status_approval"
                        android:gravity="center_vertical"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_conApproveReqAmt"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_toRightOf="@+id/txt_leavenameTitle"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="Req. Amount"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="25dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Type"
                        android:textStyle="bold"
                        android:textColor="@color/heading_dark"
                        android:textSize="@dimen/status_approval"
                        android:gravity="center_vertical"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_conApproveType"
                        android:layout_width="0dip"
                        android:layout_height="25dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_toRightOf="@+id/txt_leavenameTitle"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="Type"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_marginRight="10dp"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="5"
                android:orientation="vertical"
                android:weightSum="8">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:orientation="vertical"
                    android:weightSum="8">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
