<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="5"
                android:orientation="vertical"
                android:weightSum="8">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_punch_token"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Token No."
                        android:textColor="@color/my_custom_color"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/cor_Token_value"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="0dp"
                        android:layout_toRightOf="@+id/txt_leavenameTitle"
                        android:layout_weight="5"
                        android:text="Token Value"
                        android:textColor="@color/my_custom_color"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Comp Off Date"
                        android:textColor="@color/my_custom_color"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_corDate_value"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="0dp"
                        android:layout_toRightOf="@+id/txt_leavenameTitle"
                        android:layout_weight="5"
                        android:text="Comp Off Date"
                        android:textColor="@color/my_custom_color"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Time In"
                        android:textColor="@color/my_custom_color"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_corTimeIn_value"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Time In"
                        android:textColor="@color/my_custom_color"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Time Out"
                        android:textColor="@color/my_custom_color"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_corTimeOut_value"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="2dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Time Out"
                        android:textColor="@color/my_custom_color"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_marginRight="10dp"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="5"
                android:orientation="vertical"
                android:weightSum="8">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:orientation="vertical"
                    android:weightSum="8">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="2dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:text="Reason"
                            android:textColor="@color/my_custom_color"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_cor_reason"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:text="Reason"
                            android:textColor="@color/my_custom_color"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="2dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:text="Requested Days"
                            android:textColor="@color/my_custom_color"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_cor_requestedDays"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_toRightOf="@+id/txt_leavenameTitle"
                            android:layout_weight="5"
                            android:text="Req. Days"
                            android:textColor="@color/my_custom_color"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="2dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:text="Status"
                            android:textColor="@color/my_custom_color"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_cor_status"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_toRightOf="@+id/txt_leavenameTitle"
                            android:layout_weight="5"
                            android:text="Status"
                            android:textColor="@color/my_custom_color"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="2dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:text="Approved Days"
                            android:textColor="@color/my_custom_color"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />


                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/cor_approved_days"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Approved Days"
                            android:textColor="@color/my_custom_color"
                            android:textSize="10dp"
                            app:font="@string/font_name_light" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginTop="10dp"
            android:layout_gravity="right">

            <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                android:id="@+id/txt_cor_pullback"
                android:layout_width="80dp"
                android:layout_height="26dip"
                android:layout_marginRight="22dp"
                android:layout_marginTop="0dp"
                android:layout_toRightOf="@+id/txt_leavenameTitle"
                android:textStyle="bold"
                android:visibility="visible"
                android:gravity="center"
                android:text="Pullback"
                android:textColor="#fff"
                android:background="@drawable/button_border"
                android:textSize="12dp"/>

            <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                android:id="@+id/txt_cor_cancel"
                android:layout_width="80dp"
                android:layout_height="26dip"
                android:layout_marginRight="22dp"
                android:layout_marginTop="0dp"
                android:layout_toRightOf="@+id/txt_leavenameTitle"
                android:visibility="gone"
                android:gravity="center"
                android:text="Cancel"
                android:textStyle="bold"
                android:background="@drawable/button_border_cancel"
                android:textColor="#fff"
                android:textSize="12dp"/>
        </RelativeLayout>

        <ImageView

            android:id="@+id/img_line"
            android:layout_width="match_parent"
            android:layout_marginTop="3dp"
            android:layout_gravity="top"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_height="0.1dp"
            android:background="@color/color_red"
            />
    </LinearLayout>


</RelativeLayout>
