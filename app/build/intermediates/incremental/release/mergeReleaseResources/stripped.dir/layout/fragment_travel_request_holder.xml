<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context="com.savvy.hrmsnewapp.fragment.ODHolderFragment">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/coordinatorLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#fff">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:weightSum="10">

                <LinearLayout
                    android:id="@+id/travel_RequestLinear1"
                    android:layout_width="0dip"
                    android:layout_height="35dip"
                    android:layout_weight="5"
                    android:background="#fff"
                    android:orientation="vertical"
                    android:weightSum="3">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/travel_RequestTab1"
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
                        android:layout_weight="2.8"
                        android:background="#fff"
                        android:gravity="center"
                        android:text="Travel Request"
                        android:textColor="@color/my_custom_color"
                        android:textSize="14dp"
                        android:textStyle="bold" />

                    <ImageView
                        android:id="@+id/travel_lineRequest1"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="0.2"
                        android:background="@color/colorPrimaryDark"
                        android:visibility="visible" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/travel_StatusLinear2"
                    android:layout_width="0dip"
                    android:layout_height="35dip"
                    android:layout_weight="5"
                    android:background="#fff"
                    android:orientation="vertical"
                    android:weightSum="3">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/travel_StatusTab2"
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
                        android:layout_weight="2.8"
                        android:background="#fff"
                        android:gravity="center"
                        android:text="Travel Status"
                        android:textColor="@color/my_custom_color"
                        android:textSize="14dp"
                        android:textStyle="bold" />

                    <ImageView
                        android:id="@+id/travel_LineImageStatus2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_gravity="bottom"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_weight="0.2"
                        android:background="@color/colorPrimaryDark"
                        android:foregroundGravity="bottom"
                        android:visibility="visible" />

                </LinearLayout>


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="9">

                <FrameLayout
                    android:id="@+id/frame_travel_holder"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#fff">

                </FrameLayout>

            </LinearLayout>


        </LinearLayout>


    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</RelativeLayout>
