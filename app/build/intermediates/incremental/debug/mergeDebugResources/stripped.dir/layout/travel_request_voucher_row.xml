<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="10">

            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="5"
                android:orientation="vertical"
                android:weightSum="8">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="Token No."
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_TRVoucherToken"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="Token No."
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="Start Date"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_TRVoucherStartDate"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_toRightOf="@+id/txt_leavenameTitle"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="Start Date"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:text="Est. Cost"
                        android:gravity="center_vertical"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_TRVoucherEstCost"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="Est. Cost"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="App. Amount"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                    <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                        android:id="@+id/txt_TRVoucherAppAmt"
                        android:layout_width="0dip"
                        android:layout_height="26dip"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="5"
                        android:gravity="center_vertical"
                        android:text="App. Amount"
                        android:textColor="@color/my_custom_color"
                        android:textSize="@dimen/status_approval"
                        app:font="@string/font_name_light" />

                </LinearLayout>

                <!--<LinearLayout-->
                    <!--android:layout_width="match_parent"-->
                    <!--android:layout_height="match_parent"-->
                    <!--android:layout_marginTop="10dp"-->
                    <!--android:layout_weight="2"-->
                    <!--android:orientation="horizontal"-->
                    <!--android:weightSum="10">-->

                    <!--<com.savvy.hrms.customwidgets.CustomTextView-->
                        <!--android:layout_width="0dip"-->
                        <!--android:layout_height="25dip"-->
                        <!--android:layout_marginLeft="5dp"-->
                        <!--android:layout_marginTop="0dp"-->
                        <!--android:layout_weight="5"-->
                        <!--android:text="Expense"-->
                        <!--android:textColor="@color/my_custom_color"-->
                        <!--android:textSize="10dp"-->
                        <!--app:font="@string/font_name_light" />-->

                    <!--<com.savvy.hrms.customwidgets.CustomTextView-->
                        <!--android:id="@+id/txt_TRVoucherExpense"-->
                        <!--android:layout_width="0dip"-->
                        <!--android:layout_height="25dip"-->
                        <!--android:layout_marginLeft="5dp"-->
                        <!--android:layout_marginTop="0dp"-->
                        <!--android:layout_weight="5"-->
                        <!--android:gravity="center_vertical"-->
                        <!--android:text="Expense"-->
                        <!--android:textColor="@color/my_custom_color"-->
                        <!--android:textSize="10dp"-->
                        <!--app:font="@string/font_name_light" />-->

                <!--</LinearLayout>-->


            </LinearLayout>

            <LinearLayout
                android:layout_marginRight="10dp"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="5"
                android:orientation="vertical"
                android:weightSum="10">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:orientation="vertical"
                    android:weightSum="8">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/customTextView2"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Travel Type"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_TRVoucherTravelType"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Travel Type"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Return Date"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_TRVoucherReturnDate"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_toRightOf="@+id/txt_leavenameTitle"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Return Date"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Advance Amt"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_TRVoucherAdvAmt"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_toRightOf="@+id/txt_leavenameTitle"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Advance Amt"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Remarks"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                        <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                            android:id="@+id/txt_TRVoucherRemarks"
                            android:layout_width="0dip"
                            android:layout_height="26dip"
                            android:layout_marginLeft="5dp"
                            android:layout_marginTop="0dp"
                            android:layout_toRightOf="@+id/txt_leavenameTitle"
                            android:layout_weight="5"
                            android:gravity="center_vertical"
                            android:text="Remarks"
                            android:textColor="@color/my_custom_color"
                            android:textSize="@dimen/status_approval"
                            app:font="@string/font_name_light" />

                    </LinearLayout>


                </LinearLayout>


            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_weight="2"
            android:gravity="right"
            android:orientation="horizontal">

            <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                android:id="@+id/txt_TRVExpense"
                android:layout_width="100dip"
                android:layout_height="26dip"
                android:layout_marginTop="0dp"
                android:text="Expense"
                android:textStyle="bold"
                android:gravity="center"
                android:background="@drawable/button_border"
                android:textColor="#fff"
                android:textSize="@dimen/text_size" />

            <com.savvy.hrmsnewapp.customwidgets.CustomTextView
                android:id="@+id/txt_TRVAction"
                android:layout_width="100dip"
                android:layout_height="26dip"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:background="@drawable/button_border"
                android:text="Action"
                android:textColor="#fff"
                android:textStyle="bold"
                android:textSize="@dimen/text_size" />

        </LinearLayout>


        <ImageView

            android:id="@+id/img_line"
            android:layout_width="match_parent"
            android:layout_marginTop="5dp"
            android:layout_gravity="top"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_height="0.1dp"
            android:background="@color/color_red"
            />
    </LinearLayout>


</RelativeLayout>
